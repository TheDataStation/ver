{
  "geoCoordinate": [
    "^POINT\\(([-+]?[0-9]*\\.?[0-9]+)[ ,]+([-+]?[0-9]*\\.?[0-9]+)\\)$",
    "^\\((-?\\d+(\\.\\d+)?(.)?(°)?)[ ,]+(-?\\d+(\\.\\d+)?(.)?(°)?)\\)$"
  ],
  "street": [
    "^[\\w0-9]+ (?i)(AVE|BVD|DR|ST|RD)$",
    "^\\d+\\s*(?i)(N|NORTH|SOUTH|S|EAST|E|WEST|W)(?:.|\\s)\\s*(\\w|\\d| )+$"
  ],
  "zipCode": [
    "^\\d{5}$"
  ],
  "state": [
    "^[A-Z]{2}$"
  ]
}
